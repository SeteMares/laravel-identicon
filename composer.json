{
    "name": "irazasyed/laravel-identicon",
    "description": "Laravel Identicon Package",
    "keywords": ["identicon", "avatar", "image", "laravel identicon", "laravel"],
    "homepage": "https://irazasyed.github.io/laravel-identicon/",
    "license": "MIT",
    "authors": [
        {
            "name": "Syed I.R",
            "email": "syed@lukonet.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "*",
        "yzalis/identicon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Irazasyed\\LaravelIdenticon\\": "src/"
        }
